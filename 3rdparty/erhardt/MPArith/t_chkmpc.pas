    ln3: tbrtest = (x:   -2; y: -1e-9; u:  0.69314718055994530954; v: -3.1415926530897932385);
  ln1p1: tbrtest = (x:   -4; y:     0; u:  1.0986122886681096914;  v:  3.1415926535897932385);
