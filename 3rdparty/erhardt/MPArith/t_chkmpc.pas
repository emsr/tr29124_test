        writeln(i:4, '  ',r:20, ' r', '  ',r/eps_d:6:2);
      end
      else writeln(i:4, '  ',r:20, ' a', '  ',r/eps_d:6:2);
    {$else}
      if af>0.5 then r := r/af;
    {$endif}
    if r>ml then ml := r;
  end;
  {$ifndef fullwrite}
     writeln(' ',ml/eps_d:6:2);
  {$endif}
  if ml>maxr then maxr := ml;
end;


